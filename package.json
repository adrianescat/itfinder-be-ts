{
  "name": "itfinder-be-ts",
  "version": "1.0.0",
  "engines": {
    "node": "18.15.0",
    "npm": "9.5.0"
  },
  "description": "itfinder back-end",
  "main": "./src/index.ts",
  "scripts": {
    "dev": "NODE_ENV=development npm-run-all --parallel start:dev build:watch",
    "start:dev": "nodemon ./build/index.js --inspect --watch ./build",
    "build:watch": "NODE_ENV=development tsc -p tsconfig.json --watch",
    "start": "NODE_ENV=production node ./build/index.js",
    "build": "NODE_ENV=production tsc -p tsconfig.json",
    "lint": "eslint --ext .js,.ts .",
    "prettier:format": "prettier --write \"./**/*.ts\"",
    "prettier:check": "prettier --check \"./**/*.ts\""
  },
  "author": "Adrian Escat - @adrianescat (github)",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/dotenv-safe": "^8.1.2",
    "@types/node": "^18.16.2",
    "@types/pg": "^8.6.6",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "pg": "^8.10.0",
    "dotenv-safe": "^8.2.0",
    "fastify": "^4.17.0"
  }
}
